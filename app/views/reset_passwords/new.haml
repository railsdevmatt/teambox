%h2= t('.title')
= form_for(@reset_password) do |f|
  = errors_for @reset_password, :email
  .login.text_field.si-input-bx
    %label.login-label Email
    = f.text_field 'email', :class => 'focus login-input', :tabindex => 200
  .forgot
    %a{:href => login_path, :tabindex => "203"} Go Back
  = submit_tag t('.submit')